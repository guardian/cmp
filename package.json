{
  "name": "@guardian/consent-management-platform",
  "version": "0.0.0-this-never-updates-in-source-code-refer-to-git-tags",
  "description": "Consent management for *.theguardian.com.",
  "homepage": "https://github.com/guardian/consent-management-platform.git",
  "repository": "https://github.com/guardian/consent-management-platform.git",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm-run-all clean --parallel compile types",
    "clean": "npm-run-all --parallel clean:*",
    "clean:dist": "rm -rf dist/*",
    "clean:test-page": "rm -rf test-page/dist/*",
    "compile": "rollup --config",
    "dev": "yarn clean && yarn compile --watch",
    "fix": "yarn lint --fix",
    "lint": "eslint . --ext=js,ts,tsx",
    "prepublishOnly": "yarn build",
    "readme-toc:commit": "git add README.md && git commit -m 'Update README toc' || true",
    "readme-toc:generate": "markdown-toc README.md -i --maxdepth=3",
    "release": "np",
    "start": "yarn build && serve test-page/dist",
    "test": "npm-run-all test:unit test:e2e",
    "test:e2e": "start-server-and-test 'yarn test:e2e:serve' http://localhost:10001 'yarn cypress run'",
    "test:e2e:serve": "rollup --config --watch",
    "test:e2e:ui": "start-server-and-test 'yarn test:e2e:serve --watch' http://localhost:10001 'cypress open'",
    "test:unit": "jest --coverage",
    "tsc": "tsc",
    "types": "tsc --project ./tsconfig.build.json",
    "validate": "npm-run-all tsc test lint build"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm-run-all readme-toc:generate readme-toc:commit"
    }
  },
  "prettier": "@guardian/prettier",
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@babel/runtime": "7.12.5",
    "@cypress/skip-test": "2.5.0",
    "@guardian/eslint-config": "0.4.0",
    "@guardian/eslint-config-typescript": "0.4.1",
    "@guardian/libs": "^1.6.2",
    "@guardian/prettier": "0.4.1",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "16.0.0",
    "@rollup/plugin-html": "0.2.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/plugin-strip": "2.0.0",
    "@rollup/plugin-typescript": "6.0.0",
    "@semantic-release/github": "7.1.1",
    "@types/jest": "26.0.15",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "cypress": "5.6.0",
    "cypress-wait-until": "1.7.1",
    "eslint": "7.13.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-svelte3": "2.7.3",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "markdown-toc": "1.2.0",
    "np": "7.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "prettier-plugin-svelte": "1.4.1",
    "rollup": "2.38.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-serve": "1.1.0",
    "rollup-plugin-svelte": "6.1.1",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "17.2.3",
    "serve": "11.3.2",
    "start-server-and-test": "1.11.5",
    "svelte": "3.29.6",
    "tslib": "2.0.3",
    "typescript": "4.0.5",
    "wait-for-expect": "3.0.2"
  },
  "peerDependencies": {
    "@guardian/libs": "^1.6.2"
  }
}
