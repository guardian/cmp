{
  "name": "@guardian/consent-management-platform",
  "version": "0.0.0-this-never-updates-in-source-code-refer-to-git-tags",
  "description": "Consent management for *.theguardian.com.",
  "homepage": "https://github.com/guardian/consent-management-platform.git",
  "repository": "https://github.com/guardian/consent-management-platform.git",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm-run-all clean --parallel compile types",
    "clean": "npm-run-all --parallel clean:*",
    "clean:dist": "rm -rf dist/*",
    "clean:test-page": "rm -rf test-page/dist/*",
    "compile": "rollup --config",
    "dev": "yarn clean && yarn compile --watch",
    "fix": "yarn lint --fix",
    "github-pages-deploy": "gh-pages -d test-page/dist -u \"github-actions-bot <support+actions@github.com>\"",
    "lint": "eslint . --ext=js,ts,tsx",
    "prepublishOnly": "yarn build",
    "readme-toc:commit": "git add README.md && git commit -m 'Update README toc' || true",
    "readme-toc:generate": "markdown-toc README.md -i --maxdepth=3",
    "release": "np",
    "start": "yarn build && serve test-page/dist",
    "test": "npm-run-all test:unit test:e2e",
    "test:e2e": "start-server-and-test 'yarn test:e2e:serve' http://localhost:10001 'yarn cypress run'",
    "test:e2e:serve": "rollup --config --watch",
    "test:e2e:ui": "start-server-and-test 'yarn test:e2e:serve --watch' http://localhost:10001 'cypress open'",
    "test:unit": "jest --coverage",
    "tsc": "tsc",
    "types": "tsc --project ./tsconfig.build.json",
    "validate": "npm-run-all tsc test lint build",
    "prepare": "husky install",
    "ci:monitoring": "yarn run node-riffraff-artifact"
  },
  "prettier": "@guardian/prettier",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7",
    "@babel/plugin-transform-runtime": "~7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@guardian/eslint-config": "^3.0.0",
    "@guardian/eslint-config-typescript": "^5.0.0",
    "@guardian/libs": "14.0.0",
    "@guardian/node-riffraff-artifact": "^0.3.2",
    "@guardian/prettier": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@semantic-release/github": "^8",
    "@types/jest": "^29.5.1",
    "@types/mime": "^3.0.1",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "~5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "axios": "^1.4.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cypress": "^12.8.1",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-eslint-comments": "^3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
		"locate-character": "^2",
    "markdown-toc": "^1",
    "np": "^7.7.0",
    "npm-run-all": "^4",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "rollup": "^2.70.1",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-svelte": "^7.1.4",
    "semantic-release": "~21.0.2",
    "serve": "^14.2.0",
    "start-server-and-test": "~2.0.0",
    "svelte": "~3.59.1",
    "tslib": "^2.5.2",
    "typescript": "~4.9.5",
    "wait-for-expect": "^3"
  },
  "peerDependencies": {
    "@guardian/libs": "^14.0.0"
  }
}
