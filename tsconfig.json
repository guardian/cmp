{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"lib": ["DOM", "ES5"],
		"module": "ESNext",
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"target": "ESNext"
	},
	"exclude": ["node_modules", "dist", "coverage"]
}
