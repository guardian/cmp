    "version": "0.2.0",
    "configurations": [
        {
            "name": "CMP Debug Monitoring - prod with tcfv2",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/monitoring",
			"runtimeExecutable": "pnpm",
			"runtimeArgs": [
				"start",
				"--env=prod",
				"--jurisdiction=tcfv2"
			],
			"console": "integratedTerminal",
			"sourceMaps": true
        },
		{
            "name": "CMP Debug Monitoring - prod with ccpa (needs VPN)",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/monitoring",
			"runtimeExecutable": "pnpm",
			"runtimeArgs": [
				"start",
				"--env=prod",
				"--jurisdiction=ccpa"
			],
			"console": "integratedTerminal",
			"sourceMaps": true
        },
		{
            "name": "CMP Debug Monitoring - prod with aus (needs VPN)",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/monitoring",
			"runtimeExecutable": "pnpm",
			"runtimeArgs": [
				"start",
				"--env=prod",
				"--jurisdiction=aus"
			],
			"console": "integratedTerminal",
			"sourceMaps": true
        }
    ]
}
