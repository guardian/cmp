{
	"name": "cdk",
	"version": "0.0.0",
	"private": true,
	"bin": {
		"cdk": "bin/cdk.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"test:dev": "jest --watch",
		"format": "prettier --write \"{lib,bin}/**/*.ts\"",
		"cdk": "cdk",
		"lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
		"synth": "cdk synth --path-metadata false --version-reporting false",
		"diff": "cdk diff --path-metadata false --version-reporting false"
	},
	"devDependencies": {
		"@guardian/eslint-config-typescript": "^6.0.0",
		"@guardian/prettier": "^4.0.0",
		"@types/jest": "^29.5.2",
		"@types/node": "20.2.5",
		"aws-cdk": "2.78.0",
		"aws-cdk-lib": "2.78.0",
		"constructs": "10.2.18",
		"eslint": "^8.42.0",
		"jest": "^29.5.0",
		"prettier": "^2.8.8",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.3",
		"typescript": "~5.1.3"
	},
	"dependencies": {
		"@guardian/cdk": "50.6.0",
		"npm": "^9.6.7",
		"source-map-support": "^0.5.20"
	},
	"prettier": "@guardian/prettier"
}
